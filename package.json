{
	"name": "kary-light-ui",
	"displayName": "Kary Light UI",
	"description": "Light VS Code theme based on Kary Foundation Light Theme",
	"version": "1.0.0",
	"publisher": "karyfoundation",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other",
		"Themes"
	],
	"preview": true,
	"icon": "images/logo.png",
	"activationEvents": [
		"*"
	],
	"main": "./src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.installCustomCSS",
				"title": "Enable Kary Light UI"
			},
			{
				"command": "extension.uninstallCustomCSS",
				"title": "Disable Kary Light UI"
			}
		]
	},
	"devDependencies": {
		"mustache": "^2.2.1",
		"ncp": "^2.0.0",
		"underscore": "^1.8.3",
		"vscode": "^0.11.0"
	},
	"dependencies": {
		"extract-zip": "^1.5.0",
		"replace-in-file": "^1.0.2",
		"request": "^2.72.0"
	}
}
